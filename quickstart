# Editable install
pip install -e .[panel]

# Validate a lattice file
lattice-base-validate examples/example-lattice.yaml

# Run lattice GUI
lattice-base-lattice-gui  # or:
panel serve -m lattice_base.lattice.panel_app --show

# Run aggregate hub (includes lattice editor tab + any plugins)
lattice-base-aggregate-gui
# or:
panel serve -m lattice_base.apps.aggregate --show
